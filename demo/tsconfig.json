{
  "compilerOptions": {
    "noEmit": true,
    "target": "es2020",
    "module": "NodeNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
