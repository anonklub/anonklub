{
  "name": "@anonklub/discord-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.compile.json",
    "start": "NODE_ENV=production NODE_PATH=dist node -r module-alias/register dist",
    "start.dev": "tsnd --cls --exit-child --ignore-watch node_modules --quiet  --respawn --rs --transpile-only -r tsconfig-paths/register src",
    "typecheck": "tsc"
  },
  "_moduleAliases": {
    "~": "dist/lib"
  },
  "author": "sripwoud",
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "pino": "^8.14.1",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "snarkjs": "^0.7.0",
    "tslib": "^2.6.1",
    "typedi": "^0.10.0",
    "viem": "^1.5.0"
  },
  "engines": {
    "node": ">=19"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "typescript": "^5.1.6"
  },
  "private": true
}
